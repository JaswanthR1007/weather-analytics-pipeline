# OpenWeatherMap API Configuration
WEATHER_API_KEY=your_api_key_here

# PostgreSQL Data Warehouse Configuration
POSTGRES_DW_HOST=postgres-dw
POSTGRES_DW_PORT=5432
POSTGRES_DW_DB=weather_dw
POSTGRES_DW_USER=dataeng
POSTGRES_DW_PASSWORD=dataeng123

# Airflow Configuration
AIRFLOW_UID=50000
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:airflow@postgres-airflow/airflow

# Pipeline Configuration
PIPELINE_SCHEDULE=0 2 * * *  # Daily at 2 AM UTC
MAX_RETRIES=3
RETRY_DELAY_MINUTES=5

# Logging
LOG_LEVEL=INFO